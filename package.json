  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "axios": "^1.4.0",
    "pinia": "^2.1.3",
    "vee-validate": "^4.9.5",
    "vue": "^3.3.4",
    "vue-multiselect": "3.0.0-alpha.2",
    "vue-router": "^4.2.1",
    "vuetify": "^3.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.0",
    "@types/jsdom": "^20.0.1",
    "@types/node": "^18.16.14",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.1.3",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.0",
    "jsdom": "^20.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "rollup-plugin-visualizer": "^5.9.0",
    "sass": "^1.62.1",
    "typescript": "~4.7.4",
    "vite": "^4.3.8",
    "vite-plugin-checker": "^0.6.0",
    "vite-plugin-vuetify": "^1.0.2",
    "vitest": "^0.31.1",
    "vue-tsc": "^1.6.5"
