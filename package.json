{
  "name": "lo-marketing-platform-vue3",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "serve": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest --environment jsdom --root src/",
